                        android:layout_width="match_parent"
                        android:layout_height="22dp"
                        android:layout_marginHorizontal="@dimen/dp_20"
                        android:background="@null"
                        android:hint="请输入"
                        android:singleLine="true"
                        android:textColor="#2f2f2f"
                        android:textColorHint="#999999"
                        android:textSize="14dp"
                        android:inputType="phone"/>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="2dp"
                        android:layout_marginTop="10dp"
                        android:background="#F9F9F9"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_marginTop="@dimen/dp_12"
                        android:layout_marginHorizontal="@dimen/dp_20"
                        android:gravity="center_vertical">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:textColor="#D34545"
                            android:textSize="14dp"
                            android:text="*"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="#333333"
                            android:textStyle="bold"
                            android:textSize="14dp"
                            android:text="纳税人识别号"
                            android:layout_marginStart="4dp"/>
                    </LinearLayout>
                    <EditText
                        android:layout_marginTop="@dimen/dp_12"
                        android:id="@+id/editNaShuiCode"
                        android:layout_width="match_parent"
                        android:layout_height="22dp"
                        android:layout_marginHorizontal="@dimen/dp_20"
                        android:background="@null"
                        android:hint="请输入"
                        android:singleLine="true"
                        android:textColor="#2f2f2f"
                        android:textColorHint="#999999"
                        android:textSize="14dp"
                        android:inputType="text"/>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="2dp"
                        android:layout_marginTop="10dp"
                        android:background="#F9F9F9"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_marginTop="@dimen/dp_12"
                        android:layout_marginHorizontal="@dimen/dp_20"
                        android:gravity="center_vertical">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:textColor="#D34545"
                            android:textSize="14dp"
                            android:text="*"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="#333333"
                            android:textStyle="bold"
                            android:textSize="14dp"
                            android:text="营业执照"
                            android:layout_marginStart="4dp"/>
                    </LinearLayout>
                    <FrameLayout
                        android:id="@+id/yingYeZhiZhaoContainer"
                        android:layout_marginTop="10dp"
                        android:layout_width="@dimen/dp_240"
                        android:layout_height="@dimen/dp_150"
                        android:layout_gravity="center_horizontal">
                        <ImageView
                            android:id="@+id/yingYeZhiZhao"
                            android:layout_width="@dimen/dp_240"
                            android:layout_height="@dimen/dp_150"
                            android:scaleType="centerCrop"
                            android:src="@mipmap/bg_ent"/>
                        <TextView
                            android:id="@+id/yingYeZhiZhaoText"
                            android:layout_width="match_parent"
                            android:layout_height="32dp"
                            android:text="上传营业执照"
                            android:layout_gravity="bottom"
                            android:textColor="#FFFFFF"
                            android:gravity="center"/>
                    </FrameLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="2dp"
                        android:layout_marginTop="10dp"
                        android:background="#F9F9F9"/>
                </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="10dp"
                    android:background="#F9F9F9"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_marginTop="@dimen/dp_12"
                        android:layout_marginHorizontal="@dimen/dp_20"
                        android:gravity="center_vertical">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="#333333"
                            android:textStyle="bold"
                            android:textSize="14dp"
                            android:text="银行账户"/>
                    </LinearLayout>
                    <EditText
                        android:layout_marginTop="@dimen/dp_12"
                        android:id="@+id/editBankCode"
                        android:layout_width="match_parent"
                        android:layout_height="22dp"
                        android:layout_marginHorizontal="@dimen/dp_20"
                        android:background="@null"
                        android:hint="请输入"
                        android:singleLine="true"
                        android:textColor="#2f2f2f"
                        android:textColorHint="#999999"
                        android:textSize="14dp"
                        android:inputType="number"/>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="2dp"
                        android:layout_marginTop="10dp"
                        android:background="#F9F9F9"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_marginTop="@dimen/dp_12"
                        android:layout_marginHorizontal="@dimen/dp_20"
                        android:gravity="center_vertical">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="#333333"
                            android:textStyle="bold"
                            android:textSize="14dp"
                            android:text="开户行名称"/>
                    </LinearLayout>
                    <EditText
                        android:layout_marginTop="@dimen/dp_12"
                        android:id="@+id/editKaiHuHang"
                        android:layout_width="match_parent"
                        android:layout_height="22dp"
                        android:layout_marginHorizontal="@dimen/dp_20"
                        android:background="@null"
                        android:hint="请输入"
                        android:singleLine="true"
                        android:textColor="#2f2f2f"
                        android:textColorHint="#999999"
                        android:textSize="14dp"
                        android:inputType="text"/>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="2dp"
                        android:layout_marginTop="10dp"
                        android:background="#F9F9F9"/>
                </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="10dp"
                    android:background="#F9F9F9"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:id="@+id/choose_shouhuo_area_container">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_marginTop="@dimen/dp_12"
                        android:layout_marginHorizontal="@dimen/dp_20"
                        android:gravity="center_vertical">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="#333333"
                            android:textStyle="bold"
                            android:textSize="14dp"
                            android:text="收货地区"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingTop="@dimen/dp_12"
                        android:paddingBottom="10dp"
                        android:layout_marginHorizontal="@dimen/dp_20"
                        android:orientation="horizontal">
                        <TextView
                            android:id="@+id/choose_shouhuo_area_text"
                            android:layout_width="wrap_content"
                            android:layout_height="22dp"
                            android:text="请选择省市区"
                            android:singleLine="true"
                            android:textColor="#2f2f2f"
                            android:textColorHint="#999999"
                            android:textSize="14dp"/>
                        <View
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="match_parent"/>
                        <ImageView
                            android:layout_width="@dimen/dp_16"
                            android:layout_height="@dimen/dp_16"
                            android:src="@mipmap/choose_arrow_right"/>
                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="2dp"
                        android:background="#F9F9F9"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_marginTop="@dimen/dp_12"
                        android:layout_marginHorizontal="@dimen/dp_20"
                        android:gravity="center_vertical">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="#333333"
                            android:textStyle="bold"
                            android:textSize="14dp"
                            android:text="详细地址"/>
                    </LinearLayout>
                    <EditText
                        android:layout_marginTop="@dimen/dp_12"
                        android:id="@+id/editShouHuoDetail"
                        android:layout_width="match_parent"
                        android:layout_height="22dp"
                        android:layout_marginHorizontal="@dimen/dp_20"
                        android:background="@null"
                        android:hint="请输入"
                        android:singleLine="true"
                        android:textColor="#2f2f2f"
                        android:textColorHint="#999999"
                        android:textSize="14dp"
                        android:inputType="text"/>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="2dp"
                        android:layout_marginTop="10dp"
                        android:background="#F9F9F9"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_marginTop="@dimen/dp_12"
                        android:layout_marginHorizontal="@dimen/dp_20"
                        android:gravity="center_vertical">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="#333333"
                            android:textStyle="bold"
                            android:textSize="14dp"
                            android:text="收货地址简称"/>
                    </LinearLayout>
                    <EditText
                        android:layout_marginTop="@dimen/dp_12"
