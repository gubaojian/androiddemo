<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:id="@+id/content"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:context="com.zhongpin.mvvm_android.ui.login.RegisterActivity">
        <LinearLayout
            android:id="@+id/toolbar"
            style="@style/AppTheme.Toolbar"
            android:background="@color/transparent">
            <ImageView
                android:id="@+id/ivBack"
                android:layout_width="44dp"
                android:layout_height="44dp"
                android:layout_centerVertical="true"
                android:src="@mipmap/back_black" />
            <TextView
                android:id="@+id/iv_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="企业实名认证"
                android:textSize="18dp"
                android:textStyle="bold"
                android:textColor="#333333"/>
            <View
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"/>
        </LinearLayout>
        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_marginTop="@dimen/dp_12"
                        android:layout_marginHorizontal="@dimen/dp_20"
                        android:gravity="center_vertical">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:textColor="#D34545"
                            android:textSize="14dp"
                            android:text="*"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="#333333"
                            android:textStyle="bold"
                            android:textSize="14dp"
                            android:text="公司全称"
                            android:layout_marginStart="4dp"/>
                    </LinearLayout>
                    <EditText
                        android:layout_marginTop="@dimen/dp_12"
                        android:id="@+id/editCompanyName"
                        android:layout_width="match_parent"
                        android:layout_height="22dp"
                        android:layout_marginHorizontal="@dimen/dp_20"
                        android:background="@null"
                        android:hint="请输入"
                        android:singleLine="true"
                        android:textColor="#2f2f2f"
                        android:textColorHint="#999999"
                        android:textSize="14dp"
                        android:inputType="text"/>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="2dp"
                        android:layout_marginTop="10dp"
                        android:background="#F9F9F9"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_marginTop="@dimen/dp_12"
                        android:layout_marginHorizontal="@dimen/dp_20"
                        android:gravity="center_vertical">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:textColor="#D34545"
                            android:textSize="14dp"
                            android:text="*"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="#333333"
                            android:textStyle="bold"
                            android:textSize="14dp"
                            android:text="公司注册地址"
                            android:layout_marginStart="4dp"/>
                    </LinearLayout>
                    <EditText
                        android:layout_marginTop="@dimen/dp_12"
                        android:id="@+id/editCompanyAddress"
                        android:layout_width="match_parent"
                        android:layout_height="22dp"
                        android:layout_marginHorizontal="@dimen/dp_20"
                        android:background="@null"
                        android:hint="请输入"
                        android:singleLine="true"
                        android:textColor="#2f2f2f"
                        android:textColorHint="#999999"
                        android:textSize="14dp"
                        android:inputType="text"/>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="2dp"
                        android:layout_marginTop="10dp"
                        android:background="#F9F9F9"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_marginTop="@dimen/dp_12"
                        android:layout_marginHorizontal="@dimen/dp_20"
                        android:gravity="center_vertical">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:textColor="#D34545"
                            android:textSize="14dp"
                            android:text="*"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="#333333"
                            android:textStyle="bold"
                            android:textSize="14dp"
                            android:text="统一社会信用代码"
                            android:layout_marginStart="4dp"/>
                    </LinearLayout>
                    <EditText
                        android:layout_marginTop="@dimen/dp_12"
                        android:id="@+id/editXinYongCode"
                        android:layout_width="match_parent"
                        android:layout_height="22dp"
                        android:layout_marginHorizontal="@dimen/dp_20"
                        android:background="@null"
                        android:hint="请输入"
                        android:singleLine="true"
                        android:textColor="#2f2f2f"
                        android:textColorHint="#999999"
                        android:textSize="14dp"
                        android:inputType="text"/>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="2dp"
                        android:layout_marginTop="10dp"
                        android:background="#F9F9F9"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_marginTop="@dimen/dp_12"
                        android:layout_marginHorizontal="@dimen/dp_20"
                        android:gravity="center_vertical">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:textColor="#D34545"
                            android:textSize="14dp"
                            android:text="*"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="#333333"
                            android:textStyle="bold"
                            android:textSize="14dp"
                            android:text="法定代表人"
                            android:layout_marginStart="4dp"/>
                    </LinearLayout>
                    <EditText
                        android:layout_marginTop="@dimen/dp_12"
                        android:id="@+id/editFaren"
                        android:layout_width="match_parent"
                        android:layout_height="22dp"
                        android:layout_marginHorizontal="@dimen/dp_20"
                        android:background="@null"
                        android:hint="请输入"
                        android:singleLine="true"
                        android:textColor="#2f2f2f"
                        android:textColorHint="#999999"
                        android:textSize="14dp"
                        android:inputType="text"/>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="2dp"
                        android:layout_marginTop="10dp"
                        android:background="#F9F9F9"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_marginTop="@dimen/dp_12"
                        android:layout_marginHorizontal="@dimen/dp_20"
                        android:gravity="center_vertical">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:textColor="#D34545"
                            android:textSize="14dp"
                            android:text="*"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="#333333"
                            android:textStyle="bold"
                            android:textSize="14dp"
                            android:text="联系电话"
                            android:layout_marginStart="4dp"/>
                    </LinearLayout>
                    <EditText
                        android:layout_marginTop="@dimen/dp_12"
